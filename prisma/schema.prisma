datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}


model Board {
  id String @id @default(uuid())
  orgId String
  title String
  imageId String
  imageThumbUrl String 
  imageFullUrl String
  imageUserName String
  imageLinkHTML String

  createAt  DateTime @default(now())
  updateAt  DateTime @updatedAt

}